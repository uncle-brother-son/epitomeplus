<!doctype html>
<html>
  <head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, shrink-to-fit=no">
    {squarespace-headers}
  </head>
  <body id="{squarespace.page-id}">

      <script>console.log({website|json});</script>
      

    <header class="grid_ mt-2 mb-10 md:mb-20"><squarespace:navigation navigationId="siteNav" template="site-navigation" /></header>
    <main class="grid_" role="main" data-content-field="main-content">
      {squarespace.main-content}
    </main>
    <footer class="grid_"><squarespace:block-field id="footerBlocks" columns="12" /></footer>

    <!-- combo and minify scripts when not logged in -->
    <squarespace:script src="site.js" combo="{.if authenticatedAccount}false{.or}true{.end}" />
    <!-- other scripts, and footer code injection -->
    {squarespace-footers}

  </body>
</html>